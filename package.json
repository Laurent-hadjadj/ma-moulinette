{
  "license": "SEE LICENCE IN licence.md",
  "private": true,
  "scripts": {
    "lint": "eslint assets/js --ext .js",
    "lint:fix": "npm run lint -- --fix",
    "lint:export": "npm run lint -- --format json -o jslint-report.json",
    "dev-server": "encore dev-server",
    "dev": "encore dev",
    "watch": "encore dev --watch",
    "build": "encore production --progress"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true,
      "es2017": true
    },
    "plugins": [
      "dollar-sign",
      "html"
    ],
    "parserOptions": {
      "sourceType": "module"
    },
    "extends": [
      "eslint:recommended"
    ],
    "settings": {
      "html/html-extensions": [
        ".html",
        ".twig"
      ]
    },
    "rules": {
      "dollar-sign/dollar-sign": [
        2,
        "ignoreProperties"
      ]
    }
  },
  "dependencies": {
    "browser-update": "^3.3.38",
    "chart.js": "^3.8.0",
    "chartjs-adapter-moment": "^1.0.0",
    "chartjs-plugin-datalabels": "^2.0.0",
    "chartjs-plugin-zoom": "^1.2.1",
    "css-loader": "^6.7.1",
    "foundation-sites": "^6.7.4",
    "fsevents": "^2.1.3",
    "html2pdf.js": "^0.10.1",
    "jquery": "^3.6.0",
    "moment": "^2.29.3",
    "motion-ui": "^2.0.3",
    "select2": "^4.1.0-rc.0",
    "what-input": "^5.2.12"
  },
  "devDependencies": {
    "@hotwired/stimulus": "^3.0.0",
    "@symfony/stimulus-bridge": "^3.2.0",
    "@symfony/webpack-encore": "^1.8.2",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.2.5",
    "core-js": "^3.23.1",
    "eslint": "^8.17.0",
    "eslint-config-jquery": "^3.0.0",
    "eslint-plugin-dollar-sign": "^1.0.2",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-jquery": "^1.5.1",
    "file-loader": "^6.2.0",
    "jshint": "^2.13.4",
    "jslint": "^0.12.1",
    "regenerator-runtime": "^0.13.2",
    "stylelint": "^14.9.1",
    "stylelint-config-standard": "^25.0.0",
    "webpack-notifier": "^1.6.0"
  },
  "optionalDependencies": {
    "fsevents": "^2.1.3"
  },
  "name": "ma-moulinette",
  "description": "Ma Moulinette est une application qui a pour objectif de simplifier la consultation des indicateurs collectés et calculés sur la plateforme sonarqube.",
  "version": "1.3.0",
  "main": "webpack.config.js",
  "repository": {
    "type": "git",
    "url": "https://gitlab.franceagrimer.fr"
  },
  "keywords": [
    "sonar"
  ],
  "author": "Laurent HADJADJ"
}
