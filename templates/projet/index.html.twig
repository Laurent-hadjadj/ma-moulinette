{#-- fenêtres modales --#}
{# Affichage de la répartition des versions #}
<div id="modal-autre-version"
  class="reveal large" data-reveal data-close-on-click="true" data-animation-in="fade-in" data-animation-out="fade-out">
  <h2 class="claire-hand">Distribution des versions.</h2>
  <br/>
  <div class="float-center" style="width:54vw">
    <canvas id="graphique-autre-version" aria-label="Distribution des versions." role="img"></canvas>
  </div>
  <br />
  <a id="fermer-autre-version"
    class="expanded button open-sans" data-close aria-label="Close reveal" type="button">Fermer</a>
  <a class="close-button" data-close aria-label="Close reveal">&#215;</a>
</div>

{# Affichage du tableau des projets #}
<div id="modal-liste-projet"
  class="reveal" data-reveal data-close-on-click="true" data-animation-in="fade-in" data-animation-out="fade-out">
  <h2 class="claire-hand">Mes projets.</h2>
  <h4 class="claire-hand">
    <span id="affiche-total-projet"></span>
    trouvé(s).</h4>
  <table>
    <caption class="claire-hand">Liste des projets analysés.</caption>
    <thead>
      <tr>
        <th class="open-sans text-center">Projet</th>
        <th class="open-sans text-center">Favori</th>
        <th class="open-sans text-center" colspan="7">Actions</th>
      </tr>
    </thead>
    <tbody id="tableau-liste-projet" class="open-sans"></tbody>
  </table>
  <p class="open-sans-bold information-statut">Statut :
    <span id="information-texte" class="information-texte"></span>
  </p>
  <a id="fermer-liste-projet"
    class="expanded button open-sans" data-close aria-label="Close reveal" type="button">Fermer</a>
  <a class="close-button" data-close aria-label="Close reveal">&#215;</a>
</div>

{# Affichage du tableau des hotspots #}
<div id="modal-liste-hotspot"
  class="reveal" data-reveal data-close-on-click="true" data-animation-in="fade-in" data-animation-out="fade-out">
  <h2 class="claire-hand">Hotspots.</h2>
<h4 class="claire-hand"><span id="hotspot-total"></span> trouvé(s).</h4>
  <table>
    <caption class="claire-hand">Répartition des hotspots.</caption>
    <thead>
      <tr>
        <th scope="col" class="open-sans text-center note-rouge">HIGH</th>
        <th scope="col" class="open-sans text-center note-orange">MEDIUM</th>
        <th scope="col" class="open-sans text-center note-jaune">LOW</th>
      </tr>
    </thead>
    <tbody id="tableau-liste-hotspot" class="open-sans">
    </tbody>
  </table>
  <br/>
  <a id="fermer-liste-hotspot"
    class="expanded button open-sans" data-close aria-label="Close reveal" type="button">Fermer</a>
  <a class="close-button" data-close aria-label="Close reveal">&#215;</a>
</div>

{# Affichage de la répartition de la dette technique #}
<div id="modal-dette-technique"
  class="reveal" data-reveal data-close-on-click="true" data-animation-in="fade-in" data-animation-out="fade-out">
  <h2 class="claire-hand">Dette technique.</h2>
  <br/>
<table>
  <caption class="claire-hand">Répartition de la dette technique.</caption>
      <thead>
        <tr>
          <th scope="col">Type</th>
          <th scope="col">Dette</th>
        </tr>
      </thead>
      <tbody class="open-sans">
        <tr>
          <th scope="row">Fiabilité</th>
          <td id="js-dette-reliability"></td>
        </tr>
        <tr>
          <th scope="row">Sécurité</th>
          <td id="js-dette-vulnerability"></td>
        </tr>
        <tr>
          <th scope="row">Maintenanbilité</th>
          <td id="js-dette-code-smell"></td>
        </tr>
      </tbody>
    </table>
  <br/>
  <a id="fermer-dette-technique"
    class="expanded button open-sans" data-close aria-label="Close reveal" type="button">Fermer</a>
  <a class="close-button" data-close aria-label="Close reveal">&#215;</a>
</div>

{# Affichage du tableau des type par sévérité #}
<div id="modal-affiche-severite"
  class="reveal" data-reveal data-close-on-click="true" data-animation-in="fade-in" data-animation-out="fade-out">
  <h2 class="claire-hand">Détails des anomalies.</h2>
  <h4 class="claire-hand"><span id="affiche-nom-projet"></span></h4>
  <table>
    <caption class="claire-hand">Répartition des anomalies par type et sévérité.</caption>
    <thead>
      <tr>
        <th scope="col"></th>
        <th scope="col" class="open-sans text-center">Bug</th>
        <th scope="col" class="open-sans text-center">Vulnérabilité</th>
        <th scope="col" class="open-sans text-center">Mauvaise Pratique</th>
      </tr>
    </thead>
    <tbody id="tableau-liste-severite" class="open-sans">
      <tr>
        <th scope="row" class="rouge"><strong>Bloquant</strong></th>
        <td id="js-bug-blocker" class="text-center stat"></td>
        <td id="js-vulnerability-blocker" class="text-center stat"></td>
        <td id="js-code-smell-blocker" class="text-center stat"></td>
      </tr>
      <tr>
        <th scope="row" class="rouge"><strong>Critique</strong></td>
        <td id="js-bug-critical" class="text-center stat"></td>
        <td id="js-vulnerability-critical" class="text-center stat"></td>
        <td id="js-code-smell-critical" class="text-center stat"></td>
      </tr>
      <tr>
        <th scope="row" class="rouge"><strong>Majeur</strong></td>
        <td id="js-bug-major" class="text-center stat"></td>
        <td id="js-vulnerability-major" class="text-center stat"></td>
        <td id="js-code-smell-major" class="text-center stat"></td>
      </tr>
      <tr>
        <th scope="row" class="vert"><strong>Mineur</strong></th>
        <td id="js-bug-minor" class="text-center stat"></td>
        <td id="js-vulnerability-minor" class="text-center stat"></td>
        <td id="js-code-smell-minor" class="text-center stat"></td>
      </tr>
      <tr>
        <th scope="row" class="bleu"><strong>Info</strong></th>
        <td id="js-bug-info" class="text-center stat"></td>
        <td id="js-vulnerability-info" class="text-center stat"></td>
        <td id="js-code-smell-info" class="text-center stat"></td>
      </tr>
    </tbody>
  </table>
  <br/>
  <a id="fermer-liste-severite"
    class="expanded button open-sans" data-close aria-label="Close reveal" type="button">Fermer</a>
  <a class="close-button" data-close aria-label="Close reveal">&#215;</a>
</div>


