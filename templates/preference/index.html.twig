<html	lang="fr">

  {# Chargement de la section HEAD #}
  <head>
    <title>{{ title }}</title>
    {{ include('head.html.twig') }}
    {% block stylesheets %}
      {{ encore_entry_link_tags('preference') }}
    {% endblock %}
  </head>

  <body>
    {# Chargement de la section header #}
    {{ include('header.html.twig') }}
    <br>

    <header class="grid-container">
      <nav aria-label="Ici : " role="navigation">
        <ul class="claire-hand breadcrumbs">
          <li>
            <a href="/">Accueil</a>
          </li>
          <li>
            <span class="show-for-sr">Current:</span>Mes préférences</li>
        </ul>
      </nav>
    </header>

    <div class="grid-container">
      <h1 class="h4 claire-hand">Mes Informations personnelles.</h1>
      <div class="fieldset fieldset-information">
        <div
          class="grid-x">
          {# Prenom + Nom #}
          <div class="small-12 medium-1 large-1 cell">
            <img id="ma-tete" src="/build/avatar/{{avatar}}" alt="mOi"/>
          </div>
          <div class="small-12 medium-11 large-11 cell">
            <p class="open-sans prenom-nom">{{prenom}}
              {{nom}}</p>
          </div>
        </div>
        {# Adresse Mel #}
        <div class="grid-x">
          <div class="small-12 medium-1 large-1 cell">
            <svg version="1.1" xmlns="http://www.w3.org/2000/svg" viewbox="0 0 512 512" class="courriel-svg">
              <path d="M36.6 65.6C11.1 71.8-4.9 97.8 1.5 122.5c2 7.5 5.7 14.3 10.7 19.5 4.5 4.6 217.5 147.1 225.3 150.6 11.9 5.4 26 5.2 38.2-.5 9.5-4.5 218.2-144.2 223.5-149.6 24.9-25.4 11.3-68.6-24.3-77-9.4-2.2-429.1-2.1-438.3.1z"/>
              <path d="M0 288.4c0 81 .3 118.1 1.1 121.8 3.7 17.6 17.9 32 36 36.3 9.4 2.2 426.7 2.2 437.2 0 18.2-3.8 32.9-18.4 36.6-36.3 1.3-6.1 1.6-238.6.3-238-.4.1-48.7 32.3-107.4 71.5s-109.9 72.9-113.9 74.8c-11 5.6-20.4 7.7-33.9 7.7s-22.9-2.1-33.9-7.7c-4-1.9-55.2-35.6-113.9-74.8C49.5 204.5 1.2 172.3.8 172.2.3 172 0 224.3 0 288.4z"/>
            </svg>
          </div>
          <div class="small-12 medium-11 large-11 cell">
            <p class="open-sans adresse-mel">{{courriel}}</p>
          </div>
        </div>

        {# Mes privileges #}
        <div class="grid-x">
          <div class="small-12 medium-1 large-1 cell">
            <svg version="1.1" xmlns="http://www.w3.org/2000/svg" viewbox="0 0 512 512" class="role-svg">
