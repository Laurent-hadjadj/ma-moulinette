{% set title = 'La collecteuse.' %}
{% set description = 'Tableau de suivi des traitements de masse. Collete des données par portefeuilles de projets.' %}
<!DOCTYPE html>
<html lang="fr">
  {# Chargement de la section HEAD #}
  <head>
    {{ include('head.html.twig') }}
    {% block stylesheets %}
      {{ encore_entry_link_tags('batch') }}
    {% endblock %}
  </head>

  <body>
    {# Chargement de la section header #}
    {{ include('header.html.twig') }}
    <br>
    <header class="grid-container">
      <nav aria-label="Ici : " role="navigation">
        <ul class="claire-hand breadcrumbs">
          <li>
            <a href="/">Accueil</a>
          </li>
          <li>
            <span class="show-for-sr">Current:</span>Traitement</li>
        </ul>
      </nav>
    </header>

    <div class="grid-container">

      {# On affiche les messages #}
      {% for message in app.flashes('info') %}
        <div class="callout primary" data-closable>
          <p class="open-sans text-justify callout-message">{{ message }}</p>
          <button class="close-button" aria-label="Dismiss alert" type="button" data-close>
            <span aria-hidden="true">&times;</span>
          </button>
        </div>
      {% endfor %}

      <div class="grid-x">
        <p class="open-sans small-10 medium-11 cell">Collecte du
          {{date|format_datetime('full', 'short', locale='fr')}}.
        </p>
        <div class="small-2 medium-1 text-center float-center cell">
          <div class="tool-tip">
            <span id="info-bulle"
                  class="float-center bulle-info {{bulle}}">
                  {{infoNombre}}</span>
            <span id="info-bulle-tips" class="tool-tip-text-right">{{infoTips}}</span>
          </div>
        </div>
      </div>


      {# On choisi le type d'affichage #}
      <div class="button-group-option" data-grouptype="OU">
        <a class="js-automatique button bouton-automatique">Automatique</a>
        <a class="js-manuel button bouton-manuel">Manuel</a>
      </div>
      <div id="tableau-batch" class="table-scroll">
        <table>
          <caption class="claire-hand no-caption"></caption>
          <thead>
            <tr class="open-sans">
              <th colspan="1" scope="col" class="show-for-medium text-center">Exécution</th>
              <th colspan="1" scope="col" class="show-for-small-only text-center">Exéc</th>
              <th colspan="1" scope="col" class="show-for-medium text-center">Résultat</th>
              <th colspan="1" scope="col" class="show-for-small-only text-center">Statut</th>
              <th colspan="1" scope="col" class="show-for-medium text-center">Responsable</th>
              <th colspan="1" scope="col" class="show-for-small-only text-center">Auteur</th>
              <th colspan="1" scope="col" class="text-center">Job</th>
              <th colspan="1" scope="col" class="show-for-medium text-center">Portefeuille</th>
              <th colspan="1" scope="col" class="show-for-small-only text-center">Liste</th>
              <th colspan="1" scope="col" class="show-for-medium text-center">Projet</th>
              <th colspan="1" scope="col" class="show-for-small-only text-center">Nb</th>
              <th colspan="1" scope="col" class="text-center">Temps</th>
              <th colspan="1" scope="col" class="text-center"></th>
            </tr>
          </thead>
          <tbody id="mon-bo-tableau01" class="open-sans">
            {% set i = 0 %}
            {% for traitement in traitements %}
              {% set i = i + 1 %}
              {% if traitement.processus != "vide" %}
                <tr class="open-sans {{traitement.type}}">
                  <td id="execution-{{i}}" class="text-center">
                    {# I am a Robot Happy #}
