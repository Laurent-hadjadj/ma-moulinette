          <span class="show-for-small-only lien-modifier">Modifier</span>
        </p>
      </div>
    </div>

    {# Affiche le graphique #}
    <div class="grid-x">
      <br/>
      <div id="graphique-anomalie-data1" data-data1="{{data1}}"></div>
      <div id="graphique-anomalie-data2" data-data2="{{data2}}"></div>
      <div id="graphique-anomalie-data3" data-data3="{{data3}}"></div>
      <div id="graphique-anomalie-labels" data-label="{{labels}}"></div>
      <canvas class="show-for-medium" id="graphique-anomalie"
      style="position: relative; height:36vh; width:80vw">
      </canvas>
    </div>

	  {# Element 3 #}
    <div id="element3-to-print" class="html2pdf__page-break pleine-page">
      {# Répartition par par module #}
      <div class="table-scroll">
        <table>
          <caption class="claire-hand">Répartition par module.</caption>
          <thead>
            <tr>
              <th scope="col"></th>
              {% for i in 0..suivi|length-1 %}
                {# 0 = FALSE, 1 = TRUE  #}
                {% if suivi[i]["initial"]==1 %}
                  {% set reference="reference" %}
                {% else %}
                  {% set reference="null" %}
                {% endif %}
                <th scope="col" class="open-sans text-center {{reference}}">{{suivi[i]["version"]}}</th>
              {% endfor %}
            </tr>
          </thead>
          <tbody id="tableau-module" class="open-sans">
            <tr class="open-sans">
              <th scope="row" class="text-left text-noir">Date</td>
              {% for i in 0..suivi|length-1 %}
                <td class="text-center">
                  <span>{{suivi[i]["date"]|date("d/m/Y", "Europe/Paris")}}
                  </span>
                </td>
              {% endfor %}
            </tr>
            <tr>
              <th scope="row" class="text-left text-noir">Présentation</td>
