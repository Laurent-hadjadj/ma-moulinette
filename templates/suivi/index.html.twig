{% set title = 'Suivi des versions' %}
{% set description = 'Ma Moulinette - Suivi des versions.' %}
<!DOCTYPE html>
<html
	lang="fr">

	{# Chargement de la section HEAD #}
	<head>
		{{ include('head.html.twig') }}
		{% block stylesheets %}
			{{ encore_entry_link_tags('suivi') }}
		{% endblock %}
	</head>

	<body>
		{# Chargement de la section header #}
		{{ include('header.html.twig') }}
		<br/>
		<header class="grid-container">
			<nav aria-label="Ici : " role="navigation">
				<ul class="claire-hand breadcrumbs">
					<li>
						<a href="/">Accueil</a>
					</li>
					<li>
						<a href="/projet">Projet</a>
					</li>
					<li>
						<span class="show-for-sr">Current:</span>Suivi</li>
				</ul>
			</nav>
		</header>

    {# En-tête #}
		<div id="element1-to-print" class="pleine-page">
			<h2 id="js-nom" class="claire-hand" data-maven="{{mavenKey}}">{{nom}}</h2>
			<div class="cell">
				<h4 class="claire-hand">
					<span id="affiche-total-profil" class="stat">
						{% set nombre=suivi|length %}
						{% if nombre>1 %}
							{% set s="s"%}
						{% else %}
							{% set s="" %}
						{% endif %}
						{{nombre}}</span>
					version{{s}}
					trouvé{{s}}.
				</h4>
			</div>
		</div>

		{# Suivi des anomalies #}
		<div id="element2-to-print" class="pleine-page">
			<div class="table-scroll">
				<table>
					<caption class="claire-hand">Suivi des Anomalies.</caption>
					<thead>
						<tr>
							<th scope="col"></th>
							{% for i in 0..suivi|length-1 %}
								{# 0 = FALSE, 1 = TRUE  #}
								{% if suivi[i]["initial"]==1 %}
									{% set reference="reference" %}
								{% else %}
									{% set reference="null" %}
								{% endif %}
								<th scope="col" class="open-sans text-right {{reference}}">{{suivi[i]["version"]}}</th>
							{% endfor %}
						</tr>
					</thead>
					<tbody id="tableau-liste-profil" class="open-sans">
						<tr class="open-sans">
							<td class="text-left text-noir">Date</td>
							{% for i in 0..suivi|length-1 %}
								<td class="text-right">
									<span>{{suivi[i]["date"]|date("d/m/Y", "Europe/Paris")}}
									</span>
								</td>
							{% endfor %}
						</tr>
						<tr class="open-sans">
							<td class="text-left text-noir">Fiabilité</td>
							{% for i in 0..suivi|length-1 %}
								<td class="text-right">
									<span>{{suivi[i]["bug"]|format_number(locale='fr')}}</span>
									<span class="note note-{{suivi[i]["fiabilite"]|lower}}">{{suivi[i]["fiabilite"]}}</span>
								</td>
							{% endfor %}
						</tr>
						<tr class="open-sans">
							<td class="text-left text-noir">Sécurité</td>
							{% for i in 0..suivi|length-1 %}
								<td class="text-right">
									<span>{{suivi[i]["faille"]|format_number(locale='fr')}}</span>
									<span class="note note-{{suivi[i]["securite"]|lower}}">{{suivi[i]["securite"]}}</span>
								</td>
							{% endfor %}
						</tr>
						<tr class="open-sans">
							<td class="text-left text-noir">Hotspot</td>
							{% for i in 0..suivi|length-1 %}
								<td class="text-right">
                  {% if suivi[i]["nombre_hotspot"]!=-1 %}
                    <span>{{suivi[i]["nombre_hotspot"]|format_number(locale='fr')}}</span>
                  {% else %}
                    <span>&nbsp;-&nbsp;</span>
                  {% endif %}
                  <span class="note note-{{suivi[i]["note_hotspot"]|lower}}">{{suivi[i]["note_hotspot"]}}</span>
								</td>
							{% endfor %}
						</tr>
						<tr class="open-sans">
							<td class="text-left text-noir">Maintenabilité</td>
							{% for i in 0..suivi|length-1 %}
								<td class="text-right">
									<span>{{suivi[i]["mauvaise_pratique"]|format_number(locale='fr')}}</span>
									<span class="note note-{{suivi[i]["maintenabilite"]|lower}}">{{suivi[i]["maintenabilite"]}}</span>
								</td>
							{% endfor %}
						</tr>
						<tr class="open-sans">
							<td class="text-left text-noir">SupressWarning</td>
							{% for i in 0..suivi|length-1 %}
                <td class="text-right">
								{% if suivi[i]["suppress_warning"]!=-1 %}
									{{suivi[i]["suppress_warning"]|format_number(locale='fr')}}
                {% else %}
                    <span>&nbsp;-&nbsp;</span>
                {% endif %}
								</td>
							{% endfor %}
						</tr>
						<tr class="open-sans">
							<td class="text-left text-noir">No Sonar</td>
							{% for i in 0..suivi|length-1 %}
								<td class="text-right">
								{% if suivi[i]["no_sonar"]!=-1 %}
									{{suivi[i]["no_sonar"]|format_number(locale='fr')}}
                {% else %}
                    <span>&nbsp;-&nbsp;</span>
                {% endif %}
								</td>
							{% endfor %}
						</tr>
					</tbody>
				</table>
			</div>
		</div>

    {# Menu #}
    <div class="grid-x pleine-page on-remonte cell">
      {# Imprimer en PDF #}
      <div class="small-12 medium-4 large-4 cell">
