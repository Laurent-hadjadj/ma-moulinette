{
    "type": "project",
    "license": "CC-BY-NC-SA-4.0",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "api-platform/core": "3.2.1",
        "cesargb/php-log-rotation": "2.7.1",
        "doctrine/annotations": "1.14.3",
        "doctrine/dbal": "3.7.1",
        "doctrine/doctrine-bundle": "2.10.2",
        "doctrine/doctrine-migrations-bundle": "3.2.4",
        "doctrine/orm": "2.16.2",
        "easycorp/easyadmin-bundle": "4.7.7",
        "nelmio/cors-bundle": "2.3.1",
        "phpdocumentor/reflection-docblock": "5.3.0",
        "phpstan/phpdoc-parser": "1.24.2",
        "phpstan/phpstan": "1.10.39",
        "symfony/apache-pack": "1.0.1",
        "symfony/asset": "6.2.7",
        "symfony/console": "6.2.13",
        "symfony/doctrine-messenger": "6.2.12",
        "symfony/dom-crawler": "6.2.12",
        "symfony/dotenv": "6.2.8",
        "symfony/expression-language": "6.2.7",
        "symfony/filesystem": "6.2.12*",
        "symfony/flex": "2.3.3",
        "symfony/form": "6.2.13",
        "symfony/framework-bundle": "6.2.13",
        "symfony/http-client": "6.2.13",
        "symfony/intl": "6.2.13",
        "symfony/mailer": "6.2.13",
        "symfony/mime": "6.2.13",
        "symfony/monolog-bundle": "3.8.0",
        "symfony/notifier": "6.2.8",
        "symfony/process": "6.2.13",
        "symfony/property-access": "6.2.13",
        "symfony/property-info": "6.2.11",
        "symfony/proxy-manager-bridge": "6.2.7",
        "symfony/rate-limiter": "6.2.13",
        "symfony/runtime": "6.2.13",
        "symfony/security-bundle": "6.2.13",
        "symfony/serializer": "6.2.13",
        "symfony/string": "6.2.13",
        "symfony/translation": "6.2.11*",
        "symfony/twig-bundle": "6.2.7",
        "symfony/validator": "6.2.13",
        "symfony/web-link": "6.2.7",
        "symfony/webapp-meta": "1.0.0",
        "symfony/webpack-encore-bundle": "1.17.2",
        "symfony/yaml": "6.2.10",
        "twig/extra-bundle": "3.7.1",
        "twig/intl-extra": "3.7.1",
        "twig/twig": "3.7.1"
    },
    "require-dev": {
        "justinrainbow/json-schema": "5.2.13",
        "phpunit/phpunit": "9.6.13",
        "symfony/browser-kit": "6.2.7",
        "symfony/css-selector": "6.2.13",
        "symfony/debug-bundle": "6.2.13",
        "symfony/maker-bundle": "1.50.0",
        "symfony/phpunit-bridge": "6.3.2",
        "symfony/stopwatch": "6.2.7",
        "symfony/web-profiler-bundle": "6.2.13"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": false,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.2.*"
        }
    }
}
